<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>

<!-- 
    согласно правилам именования контроллера вложенного документа имя контроллера:
    fx:id + Controller = closeBtn + Controller = CloseBtnController
-->


<Button fx:id="closeBtn" onAction="#showCloseMsg" text="Close" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40" fx:controller="include.test.CloseBtnController" />
